# Cloudflare Turnstile Secret Key
# Get your secret key from: https://dash.cloudflare.com/?to=/:account/turnstile
# Copy this file to .dev.vars and replace with your actual secret key
TURNSTILE_SECRET_KEY=1x0000000000000000000000000000000AA

# Development Mode (ONLY for local development)
# Set to 'true' or '1' to allow localhost origins for CORS
# DO NOT set this in production - it will be tree-shaken out
DEV_MODE=true

# Contact Email (optional for local dev, required in production)
CONTACT_EMAIL=test@example.com